#include <lib/util.h>

float lb;
int first, max;
char string[80];
lb=15;
GETCURDATA

first = expno;
STOREPAR ("LB", lb)
GETINT ("Enter first expno to process : ",first)
max = 10;
GETINT ("Enter number of expnos : ",max)
WPAR("tmpmefp","proc")
expno = first;
TIMES(max)
  RPAR("tmpmefp","proc")
  FT
  MC
  IEXPNO
END
DEXPNO

/* delete remaining files */

DELPAR("tmpmefp")

QUITMSG("--- multiefp finished ---")